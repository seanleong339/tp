<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox id="testPanel" fx:id="testPanel" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <GridPane HBox.hgrow="ALWAYS">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="150" />
        </columnConstraints>
        <VBox alignment="CENTER_LEFT" minHeight="105" GridPane.columnIndex="0">
            <padding>
                <Insets bottom="5" left="15" right="5" top="5" />
            </padding>
            <HBox alignment="CENTER_LEFT" prefWidth="103.0" spacing="5">
                <Label fx:id="name" style="-fx-text-fill: white;" styleClass="cell_big_label" text="name holder" textFill="WHITE">
               <font>
                  <Font size="24.0" />
               </font></Label>
            </HBox>
            <FlowPane fx:id="tags" prefHeight="30.0" prefWidth="130.0" />
            <Label fx:id="phone" style="-fx-text-fill: white;" styleClass="cell_small_label" text="phone number: " />
            <Label fx:id="address" style="-fx-text-fill: white;" styleClass="cell_small_label" text="address: " />
            <Label fx:id="email" style="-fx-text-fill: white;" styleClass="cell_small_label" text="email: "/>
        </VBox>
      <rowConstraints>
         <RowConstraints />
      </rowConstraints>
    </GridPane>
</HBox>
